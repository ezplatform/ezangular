{
  "name": "ezfinhub",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "nx",
    "preinstall": "node ./scripts/preinstall.js",
    "postinstall": "husky install && node ./scripts/postinstall.js && node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "build:all": "yarn nx run-many --target=build --all --parallel --with-deps",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "===== SINGLE SPA =====": "",
    "start:portal": "nx run portal:serve --disable-host-check --port 4200 --live-reload false",
    "start:sidebar": "nx run sidebar:serve --disable-host-check --port 4201 --live-reload false",
    "start:authentication": "nx run authentication:serve --disable-host-check --port 4202 --live-reload false",
    "start:all": "concurrently --kill-others \"yarn start:portal\" \"yarn start:sidebar\" \"yarn start:authentication\"",
    "===== FIREBASE FUNCTION =====": "",
    "firebase:serve": "concurrently --kill-others \"nx run functions:build --watch\" \"firebase serve --only functions\"",
    "firebase:shell": "concurrently --kill-others \"yarn build functions --watch\" \"firebase functions:shell\" --raw",
    "firebase:deploy:mf": "firebase deploy --only functions:microFrontend",
    "firebase:deploy:ngsw": "firebase deploy --only functions:ngsw",
    "===== PWA =====": "",
    "ngsw:portal": "node ./scripts/ngsw-regen.js"
  },
  "dependencies": {
    "@angular/animations": "12.1.3",
    "@angular/cdk": "12.1.3",
    "@angular/common": "12.1.3",
    "@angular/compiler": "12.1.3",
    "@angular/core": "12.1.3",
    "@angular/fire": "6.1.5",
    "@angular/forms": "12.1.3",
    "@angular/platform-browser": "12.1.3",
    "@angular/platform-browser-dynamic": "12.1.3",
    "@angular/router": "12.1.3",
    "@angular/service-worker": "12.1.3",
    "js-beautify": "1.14.0",
    "ng-zorro-antd": "12.0.1",
    "posthtml": "0.16.4",
    "posthtml-attrs-sorter": "1.1.0",
    "rxjs": "6.6.6",
    "single-spa-angular": "5.0.1",
    "single-spa-layout": "1.6.0",
    "tslib": "2.3.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "12.1.0",
    "@angular-devkit/build-angular": "12.1.3",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "12.1.3",
    "@angular/compiler-cli": "12.1.3",
    "@angular/language-service": "12.1.3",
    "@nrwl/angular": "12.6.2",
    "@nrwl/cli": "12.6.2",
    "@nrwl/cypress": "12.6.2",
    "@nrwl/eslint-plugin-nx": "12.6.2",
    "@nrwl/jest": "12.6.2",
    "@nrwl/linter": "12.6.2",
    "@nrwl/node": "12.6.2",
    "@nrwl/nx-cloud": "12.3.0",
    "@nrwl/tao": "12.6.2",
    "@nrwl/workspace": "12.6.2",
    "@types/jest": "26.0.24",
    "@types/jsdom": "16.2.13",
    "@types/node": "16.4.3",
    "@types/systemjs": "6.1.1",
    "@types/tough-cookie": "4.0.1",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "@typescript-eslint/parser": "4.28.4",
    "axios": "0.21.1",
    "axios-cookiejar-support": "1.0.1",
    "concurrently": "6.2.0",
    "csvtojson": "2.0.10",
    "cypress": "8.0.0",
    "dotenv": "10.0.0",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unused-imports": "1.1.2",
    "firebase": "8.8.0",
    "firebase-admin": "9.11.0",
    "firebase-functions": "3.14.1",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.2",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "jest-preset-angular": "9.0.5",
    "jsdom": "16.6.0",
    "lint-staged": "11.1.1",
    "prettier": "2.3.2",
    "pretty-quick": "3.1.1",
    "sha256": "0.2.0",
    "tough-cookie": "4.0.0",
    "ts-jest": "27.0.4",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md,scss}": [
      "pretty-quick --staged",
      "git add -f"
    ],
    "*.html": [
      "node ./scripts/html-formatter.js --staged",
      "git add -f"
    ]
  }
}
