{
  "version": 2,
  "projects": {
    "investor": {
      "projectType": "application",
      "root": "apps/investor",
      "sourceRoot": "apps/investor/src",
      "prefix": "ezfinhub",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/investor",
            "index": "apps/investor/src/index.html",
            "main": "apps/investor/src/main.ts",
            "polyfills": "apps/investor/src/polyfills.ts",
            "tsConfig": "apps/investor/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/investor/src/favicon.ico",
              "apps/investor/src/assets"
            ],
            "styles": [
              "apps/investor/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/investor/src/environments/environment.ts",
                  "with": "apps/investor/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "investor:build:production"
            },
            "development": {
              "browserTarget": "investor:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "investor:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/investor/src/**/*.ts",
              "apps/investor/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/investor"
          ],
          "options": {
            "jestConfig": "apps/investor/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "investor-api": {
      "root": "apps/investor-api",
      "sourceRoot": "apps/investor-api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/investor-api",
            "main": "apps/investor-api/src/main.ts",
            "tsConfig": "apps/investor-api/tsconfig.app.json",
            "assets": [
              "apps/investor-api/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/investor-api/src/environments/environment.ts",
                  "with": "apps/investor-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "investor-api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/investor-api/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/investor-api"
          ],
          "options": {
            "jestConfig": "apps/investor-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
